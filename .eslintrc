{
  "extends": "react-app",
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "react/jsx-filename-extension": 0,
    "no-use-before-define": [
      "error",
      {
        "functions": false
      }
    ]
  }
}